---
import { twMerge } from "tailwind-merge";

interface Props {
  class?: string;
  children: any;
}

const props = Astro.props;
---

<svg
  width="1em"
  height="1em"
  viewBox="0 0 16 16"
  class={twMerge("h-auto w-[18px] fill-subtle", props.class)}
  xmlns="http://www.w3.org/2000/svg"
>
  <slot />
</svg>
